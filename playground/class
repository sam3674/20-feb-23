class Schedule:
    def __init__(self):
        self.schedule = {}
    def add_task(self, task_name, start_time, end_time):
        if start_time >= end_time:
            print("Error: Invalid start and end time")
            return
        if start_time in self.schedule:
            print("Error: Task already exists at this time")
            return
        self.schedule[start_time] = (task_name, end_time)
        print("Task added successfully")
    def remove_task(self, start_time):
        if start_time not in self.schedule:
            print("Error: Task not found")
            return
        del self.schedule[start_time]
        print("Task removed successfully")
    def view_schedule(self):
        if not self.schedule:
            print("No tasks scheduled")
            return
        print("Task Name\tStart Time\tEnd Time")
        for start_time in sorted(self.schedule.keys()):
            task_name, end_time = self.schedule[start_time]
            print("{}\t\t{}\t\t{}".format(task_name, start_time, end_time))
 
 
schedule = Schedule()
schedule.add_task("Meeting with John", 10, 11)
schedule.add_task("Call with Jane", 11, 12)

schedule.view_schedule()
schedule.remove_task(10)
schedule.view_schedule()
